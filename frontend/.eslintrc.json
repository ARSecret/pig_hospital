{
    "root": true,
    "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "@vue/eslint-config-prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        // "indent": ["error", 4],
        "comma-dangle": ["error", "always-multiline"],
        "no-unused-vars": ["warn"],
        "vue/require-prop-types": ["off"]
    }
}
